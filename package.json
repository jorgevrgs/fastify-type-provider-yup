{
  "name": "fastify-type-provider-yup",
  "version": "0.0.4",
  "description": "Yup type provider for Fastify v4",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "scripts": {
    "build": "pnpm build:esm && pnpm build:cjs",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "dev:cjs": "tsc-watch --noClear -p ./tsconfig.cjs.json --onSuccess \"node ./examples/plugin-common.example.cjs\"",
    "format": "biome format --write .",
    "lint": "biome lint .",
    "lint:fix": "biome check --write .",
    "test": "pnpm test:js && pnpm test:ts",
    "test:js": "vitest",
    "test:ts": "vitest --typecheck"
  },
  "keywords": [
    "fastify",
    "yup",
    "type-providers",
    "schemas",
    "validation",
    "serialization"
  ],
  "author": "Jorge Vargas <jorgevargasla@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.22.17",
    "@biomejs/biome": "1.8.2",
    "@fastify/swagger": "^8.10.0",
    "@fastify/swagger-ui": "^1.9.3",
    "@tsconfig/node-lts": "^18.12.5",
    "@types/json-schema": "^7.0.12",
    "fastify": "^4.23.0",
    "fastify-plugin": "^4.5.1",
    "json-schema": "^0.4.0",
    "pino-pretty": "^10.2.0",
    "ts-node": "^10.9.1",
    "tsc-watch": "^6.0.4",
    "typescript": "^5.2.2",
    "vitest": "^1.6.0",
    "yup": "^1.2.0"
  },
  "peerDependencies": {
    "fastify": "^4.23.0",
    "yup": "^1.2.0"
  },
  "dependencies": {
    "@sodaru/yup-to-json-schema": "^2.0.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jorgevrgs/fastify-type-provider-yup"
  },
  "bugs": {
    "url": "https://github.com/jorgevrgs/fastify-type-provider-yup/issues"
  },
  "homepage": "https://github.com/jorgevrgs/fastify-type-provider-yup#readme",
  "files": ["dist"]
}
